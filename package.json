{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "Boiler plate for express applications with user login and authentication",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm start\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engine": {
    "node": "10.16.3"
  },
  "keywords": [
    "boilerplate"
  ],
  "author": "Eric Chung",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "dotenv": "8.1.0",
    "express": "4.17.1",
    "express-validator": "6.1.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "8.5.1",
    "mongoose": ">=5.7.5",
    "serialize-javascript": ">=2.1.1"
  },
  "devDependencies": {
    "concurrently": "4.1.2",
    "nodemon": "1.19.1"
  }
}
